    const markeryLayer = L.featureGroup();
  
    miejscowosciMarkery.forEach(loc => {
      L.marker(loc.coords)
        .bindPopup(`<strong>${loc.name}</strong>`)
        .addTo(markeryLayer);
    });
  
    markeryLayer.addTo(map);
  
    // 6. Dopasowujemy widok do WSZYSTKICH obiektów (poligonów i markerów)
